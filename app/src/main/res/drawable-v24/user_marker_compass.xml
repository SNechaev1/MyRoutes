<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="9dp" android:right="9dp" android:top="9dp" android:bottom="9dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="104%"
            android:pivotY="27%">
            <shape android:shape="rectangle">
                <solid android:color="@color/brand_prim" />
            </shape>
        </rotate>
    </item>
    <item android:left="5dp" android:right="5dp" android:top="5dp" android:bottom="5dp">
        <shape
            android:shape="oval">
            <stroke
                android:width="3dp"
                android:color="@color/brand_prim">
            </stroke>
            <solid
                android:color="@color/white">
            </solid>
            <size
                android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>
</layer-list>